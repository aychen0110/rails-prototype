<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <link href='http://fonts.googleapis.com/css?family=Meddon' rel='stylesheet' type='text/css'>
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= data.page.title || "New Project" %></title>
    
    <%= stylesheet_link_tag "site", "demo", "magiczoom" %>
    <%= javascript_include_tag  "jquery-1.8.2.min", "jquery.simplemodal.1.4.2.min", "bootstrap.min", "modernizr", "d3.v3.min",  "magiczoom.cstreams.registered", "mousetrap.min.js", "jquery-ui-1.10.1.custom.min" ,
      "app" %>
  </head>
  
  <body class="<%= page_classes %>">
    <%= yield %>
  </body>
</html>
