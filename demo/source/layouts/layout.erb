<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <link href='http://fonts.googleapis.com/css?family=Meddon' rel='stylesheet' type='text/css'>
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= data.page.title || "New Project" %></title>

    <%= stylesheet_link_tag "site", "demo", "magiczoom" %>
    <%= javascript_include_tag  "jquery-1.8.2.min", "jquery.simplemodal.1.4.2.min", "bootstrap.min", "modernizr", "d3.v3.min",  "magiczoom.cstreams.registered", "mousetrap.min", "jquery-ui-1.10.1.custom.min" ,  "soundmanager2-nodebug-jsmin",
      "app" %>

      

  </head>
  
  <script type="text/javascript">
    soundManager.setup({
      url: '/swf/',
      flashVersion: 9, // optional: shiny features (default = 8)
      useFlashBlock: false, 
      onready: function() {
        // Ready to use; soundManager.createSound() etc. can now be called.
      }
    });
  </script>


  <body class="<%= page_classes %>">
    <%= yield %>
  </body>
</html>
